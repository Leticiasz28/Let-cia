int main() {
	int total, lado1, lado2;
	printf ("digite o lado1:");
	scanf ("%d", &lado1);
	printf ("digite o lado2:");
	scanf ("%d", &lado2);
	total = lado1+lado2;
	printf ("a area do quadrado e: %d", total);

}
